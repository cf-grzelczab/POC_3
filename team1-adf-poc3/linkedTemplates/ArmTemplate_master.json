{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "team1-adf-poc3"
		},
		"team1adfpoc3ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'team1adfpoc3ls'"
		},
		"GIOS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.gios.gov.pl/pjp-api/rest/station/sensors/"
		},
		"GIOS_rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.gios.gov.pl/pjp-api/rest/station/sensors/"
		},
		"GIOS_rest_pollutions_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.gios.gov.pl/pjp-api/rest/aqindex/getIndex/"
		},
		"team1adfpoc3ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://team1poc3dls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "team1-adf-poc3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"team1adfpoc3ls_accountKey": {
						"value": "[parameters('team1adfpoc3ls_accountKey')]"
					},
					"GIOS_properties_typeProperties_url": {
						"value": "[parameters('GIOS_properties_typeProperties_url')]"
					},
					"GIOS_rest_properties_typeProperties_url": {
						"value": "[parameters('GIOS_rest_properties_typeProperties_url')]"
					},
					"GIOS_rest_pollutions_properties_typeProperties_url": {
						"value": "[parameters('GIOS_rest_pollutions_properties_typeProperties_url')]"
					},
					"team1adfpoc3ls_properties_typeProperties_url": {
						"value": "[parameters('team1adfpoc3ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}